    "qwe": "sh ./bin/qwe.sh",
    "test": "chmod +x ./bin/test.sh & sh ./bin/test.sh",
    "dev": "npm run migration:generate src/infrastructure/db/migrations && npm run schema:sync && nodemon src/index.ts",
    "build": "tsc -p .",
    "start": "NODE_PATH=./build node build/index.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
