    "schema:sync": "npm run typeorm schema:sync -- -d src/infrastructure/db/orm/Connection.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run --dataSource src/infrastructure/db/orm/Connection.ts"
  },
